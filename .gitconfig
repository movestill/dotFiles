[user]
	email = movestill@gmail.com
	name = Tim Gion
[alias]
    st = status
    srb = svn rebase
    sdc = svn dcommit
    spop = stash pop
    ci = commit
    co = checkout
    ll = log --stat
[credential]
	helper = cache --timeout=7200
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[commit]
	template = ~/.stCommitMsg
[diff]
	tool = vimdiff
[mergetool]
	prompt = false
[merge]
	conflictstyle = diff3
	tool = vimdiff
